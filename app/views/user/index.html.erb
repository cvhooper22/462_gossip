<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th colspan="2"></th>
      <th>UUID</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <th colspan="2"></th>
        <td><%= user.uuid %></td>
      </tr>
    <% end %>
  </tbody>
</table>

